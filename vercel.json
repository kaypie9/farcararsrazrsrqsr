{
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        { "key": "X-Frame-Options", "value": "ALLOWALL" },
        { "key": "Content-Security-Policy", "value": "frame-ancestors * 'self' https://*;" }
      ]
    }
  ],
  "routes": [
    { "src": "/mini", "dest": "/mini.html" }
  ]
}
